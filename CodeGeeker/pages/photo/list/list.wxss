page {
    width: 750rpx;
}

.list {
    padding: 0 20rpx;
    width: 710rpx;
}

.list .item {
    width: 340rpx;
    display: inline-block;
    position: relative;
}

.list .item .photo {
    width: 100%;
    height: 100%;
}

.list .item .grapher {
    height: 40rpx;
    position: absolute;
    bottom: 20rpx;
    left: 20rpx;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
}

.list .item .grapher image {
    width: 40rpx;
    height: 40rpx;
    border-radius: 20rpx;
}

.list .item .grapher view {
    line-height: 40rpx;
    margin-left: 20rpx;
    font-size: 24rpx;
    color: #fff;
}

.list .item .favcount {
    line-height: 40rpx;
    position: absolute;
    bottom: 20rpx;
    right: 20rpx;
    font-size: 24rpx;
    color: #fff;
}